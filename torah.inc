function getLinkForShabbatHolidayReading($month, $day, $year) {
  if (cal_days_in_month(CAL_JEWISH, 3, $year) == 29)
    $shortKislev = true;
  else
    $shortKislev = false;

  if (torahHebrewLeapYear($year))
    $lastMonth = 13;
  else
    $lastMonth = 12;

  $result = "";

  $GENESIS = 1;
  $EXODUS = 2;
  $LEVITICUS = 3;
  $NUMBERS = 4;
  $DEUTERONOMY = 5;

  //------- Tishri
  if ($day == 1 && $month == 1) { // 1st day of Rosh Hashanah
    $result = array(getTorahSectionLinkHelper($GENESIS, 21, 1),
                    getTorahSectionLinkHelper($NUMBERS, 29, 1));
  }
  if ($day == 2 && $month == 1) { // 2nd day of Rosh Hashanah
    $result = array(getTorahSectionLinkHelper($GENESIS, 22, 1),
                    getTorahSectionLinkHelper($NUMBERS, 29, 1));
  }
  if ($day == 10 && $month == 1) { // Yom Kippur
    $result = array(getTorahSectionLinkHelper($LEVITICUS, 16, 1),
                    getTorahSectionLinkHelper($NUMBERS, 29, 7),
                    getTorahSectionLinkHelper($LEVITICUS, 18, 1));
  }
  if ($day == 15 && $month == 1) { // Sukkot I
    $result = array(getTorahSectionLinkHelper($LEVITICUS, 22, 26),
                    getTorahSectionLinkHelper($NUMBERS, 29, 12));
  }
  if ($day == 16 && $month == 1) { // Sukkot II
    $result = array(getTorahSectionLinkHelper($LEVITICUS, 22, 26),
                    getTorahSectionLinkHelper($NUMBERS, 29, 12));
  }
  if ($day == 17 && $month == 1) { // Sukkot III
    $result = array(getTorahSectionLinkHelper($EXODUS, 33, 12),
                    getTorahSectionLinkHelper($NUMBERS, 29, 17));
  }
  if ($day == 18 && $month == 1) { // Sukkot IV
    $result = array(getTorahSectionLinkHelper($NUMBERS, 29, 20));
