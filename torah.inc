  }
  if ($day == 20 && $month == 1) { // Sukkot VI
    $result = array(getTorahSectionLinkHelper($EXODUS, 33, 12),
                    getTorahSectionLinkHelper($NUMBERS, 29, 23));
  }
  if ($day == 21 && $month == 1) { // Hoshanah Rabbah
    $result = array(getTorahSectionLinkHelper($NUMBERS, 29, 26));
  }
  if ($day == 22 && $month == 1) { // Shemini Azereth
    $result = array(getTorahSectionLinkHelper($DEUTERONOMY, 14, 22),
                    getTorahSectionLinkHelper($NUMBERS, 29, 35));
  }
  if ($day == 23 && $month == 1) { // Simchat Torah
    $result = array(getTorahSectionLinkHelper($DEUTERONOMY, 33, 1),
                    getTorahSectionLinkHelper($GENESIS, 1, 1),
                    getTorahSectionLinkHelper($NUMBERS, 29, 35));
  }

  //------- Kislev/Tevet
  if ($day == 25 && $month == 3) { // Chanukkah I
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 1));
  }
  if ($day == 26 && $month == 3) { // Chanukkah II
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 18));
  }
  if ($day == 27 && $month == 3) { // Chanukkah III
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 24));
  }
  if ($day == 28 && $month == 3) { // Chanukkah IV
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 30));
  }
  if ($day == 29 && $month == 3) { // Chanukkah V
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 36));
  }
  if (($shortKislev && $day == 1 && $month == 4) ||
      (!$shortKislev && $day == 30 && $month == 3)) { // Chanukkah VI
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 42));
  }
  if (($shortKislev && $day == 2 && $month == 4) ||
      (!$shortKislev && $day == 1 && $month == 4)) { // Chanukkah VII
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 48));
  }
  if (($shortKislev && $day == 3 && $month == 4) ||
      (!$shortKislev && $day == 2 && $month == 4)) { // Chanukkah VIII
    $result = array(getTorahSectionLinkHelper($NUMBERS, 7, 54));
  }

  //------- Adar/Adar II
  if ($day == 14 && $month == $lastMonth) { // Purim
    $result = array(getTorahSectionLinkHelper($EXODUS, 17, 8));
  }

  //------- Nisan
  if ($day == 15 && $month == 8) { // Pesach I
    $result = array(getTorahSectionLinkHelper($EXODUS, 12, 21),
                    getTorahSectionLinkHelper($NUMBERS, 28, 16));
  }
  if ($day == 16 && $month == 8) { // Pesach II
    $result = array(getTorahSectionLinkHelper($LEVITICUS, 22, 26),
                    getTorahSectionLinkHelper($NUMBERS, 28, 16));
  }
  if ( (($day >= 17) && ($day <= 20)) && $month == 8) { // Hol Hamoed Pesah
    $result = array(getTorahSectionLinkHelper($EXODUS, 33, 12),
                    getTorahSectionLinkHelper($NUMBERS, 28, 19));
  }
  if ($day == 21 && $month == 8) { // Pesach VII
    $result = array(getTorahSectionLinkHelper($EXODUS, 13, 17),
                    getTorahSectionLinkHelper($NUMBERS, 28, 19));
  }
  if ($day == 22 && $month == 8) { // Pesach VIII
    $result = array(getTorahSectionLinkHelper($DEUTERONOMY, 14, 22),
                    getTorahSectionLinkHelper($NUMBERS, 28, 19));
  }

  //------- Sivan
  if ($day == 6 && $month == 10) { // Shavuot I
    $result = array(getTorahSectionLinkHelper($EXODUS, 19, 1),
                    getTorahSectionLinkHelper($NUMBERS, 28, 26));
  }
  if ($day == 7 && $month == 10) { // Shavuot II
    $result = array(getTorahSectionLinkHelper($EXODUS, 14, 22),
                    getTorahSectionLinkHelper($NUMBERS, 28, 26));
  }

  return $result;
}

